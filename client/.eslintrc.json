{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "vue/no-v-model-argument": "off",
        "vue/no-multiple-template-root": "off",
        "max-len": ["error", {"code": 180}],
        "indent": ["error", 2]
    }
}
